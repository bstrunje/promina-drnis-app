{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "frontend/dist",
  "env": {
    "VERCEL": "1"
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/[...slug]" },
    { "source": "/uploads/(.*)", "destination": "/api/[...slug]" },
    { "source": "/health", "destination": "/api/[...slug]" }
  ]
}