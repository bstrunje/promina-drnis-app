{
  "name": "promina-drnis-app",
  "type": "module",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "shared"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "npm run dev -w frontend",
    "dev:backend": "npm run dev -w backend",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "npm run preview -w frontend",
    "start:backend": "npm run start -w backend",
    "build": "npm run clean && npm run build:frontend && npm run build:backend",
    "build:frontend": "npm run build -w frontend",
    "build:backend": "npm run build -w backend",
    "_test": "npm run test:frontend && npm run test:backend",
    "_test:frontend": "npm run test -w frontend",
    "_test:backend": "npm run test -w backend",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "npm run lint -w frontend",
    "lint:backend": "npm run lint -w backend",
    "typecheck": "npm run typecheck:frontend && npm run typecheck:backend",
    "typecheck:frontend": "npm run typecheck -w frontend",
    "typecheck:backend": "npm run typecheck -w backend",
    "clean": "npm run clean:frontend && npm run clean:backend",
    "clean:frontend": "npm run clean -w frontend",
    "clean:backend": "npm run clean -w backend"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.9.0",
    "@types/sequelize": "^4.28.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-toast": "^1.2.3",
    "@types/multer": "^1.4.12",
    "@types/react": "^18.3.18",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.1.0",
    "react": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "sequelize": "^6.37.5",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.5"
  }
}
