{
    "version": 2,
    "framework": null,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
  "public": true,
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/(.*)", "dest": "http://localhost:3000/api/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }